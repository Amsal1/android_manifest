    <linkfile src="os_pickup.mk" dest="vendor/nxp/opensource/sn100x/Android.mk" />
  </project>

  <!-- Framework repos -->
  <project path="frameworks/av" name="android_frameworks_av" remote="ten" />
  <project path="frameworks/base" name="android_frameworks_base" remote="ten" />
  <project path="frameworks/native" name="android_frameworks_native" remote="ten" />
