<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote name="ten"
          fetch="https://github.com/Cherish-Future"
          revision="ten" />

  <remote name="CherishOS-Devices"
          fetch="https://github.com/CherishOS-Devices"
          revision="ten" />

  <remote name="pixel-gitlab"
          fetch="https://gitlab.com/PixelExperience"
